extends ../layout

block content
	h1 #{round}
	h4 #{year} #{tournament} 
		span.label.label-important #{type}
	if session.email
		a.btn.addnew(href="/new?type=#{type}&year=#{year}&tournament=#{tournament}") Add New Packet
	p
		each question, num in entries
			ul.breadcrumb
				li
					//- span.label= question.num
					li= question.num
						span.divider /	
					| &nbsp;
				li= question.category
				li.pull-right= question.answer
			div.well= question.question

extends ../layout

block content
	ol.breadcrumb
		li
			a(href="/packets/"+type)= type
			span.divider /
		li
			a(href="/packets/"+type+"/"+year)= year
			span.divider /
		li
			a(href="/packets/"+type+"/"+year+"/"+tournament)= tournament
		
		if session.email
			li.pull-right
				a(href="/new?type=#{type}&year=#{year}&tournament=#{tournament}") Add New Packet
		

	p
		each round in rounds
			li
				a(href="/packets/#{type}/#{year}/#{encodeURIComponent(tournament)}/#{encodeURIComponent(round)}")= round
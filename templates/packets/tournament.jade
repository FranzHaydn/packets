extends ../layout

block content
	h1 #{year} #{tournament} 
		span.label.label-important #{type} 
	if session.email
		a.btn.addnew(href="/new?type=#{type}&year=#{year}&tournament=#{tournament}") Add New Packet
	p
		each round in rounds
			li
				a(href="/packets/#{type}/#{year}/#{encodeURIComponent(tournament)}/#{encodeURIComponent(round)}")= round